{
  "name": "nuxt-quick-start",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "ad": "git add .",
    "push": "git add . && git commit && git push",
    "lint:js": "yarn lint:js",
    "lint": "eslint --ext \".js,.vue\" --ignore-path .gitignore . --fix"
  },
  "dependencies": {
    "core-js": "^3.15.1",
    "element-ui": "^2.15.2",
    "nuxt": "^2.15.7"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": "eslint --ext \".js,.vue\" --ignore-path .gitignore . --fix"
  }
}